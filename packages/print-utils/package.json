{
  "name": "print-utils",
  "description": "Print utilities for client web applications.",
  "version": "0.2.4",
  "main": "lib/index.js",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "start": "npm run build -- --watch",
    "prebuild": "rimraf lib dist",
    "build:dist": "rollup -c --sourcemap inline --environment NODE_ENV:production",
    "build": "babel src -d lib --ignore __tests__,__mocks__",
    "preversion": "npm run build && npm run build:dist",
    "test": "jest"
  },
  "devDependencies": {
    "babel-cli": "latest",
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-plugin-transform-runtime": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-latest": "latest",
    "babel-preset-react": "latest",
    "babel-preset-stage-0": "latest",
    "babel-register": "latest",
    "babelrc-rollup": "^3.0.0",
    "bin-utils": "latest",
    "eslint": "latest",
    "eslint-plugin-babel": "latest",
    "ncp": "latest",
    "npm-run-all": "latest",
    "rimraf": "latest",
    "rollup": "latest",
    "rollup-plugin-babel": "latest",
    "rollup-plugin-commonjs": "latest",
    "rollup-plugin-node-resolve": "latest",
    "rollup-watch": "latest",
    "webpack": "^2.1.0-beta.25"
  },
  "dependencies": {
    "browser-detective": "^0.1.7",
    "raf": "^3.3.0"
  }
}
